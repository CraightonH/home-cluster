{
    "files.associations": {
        "*.json5": "jsonc",
        "./kubernetes/**/*.sops.toml": "plaintext"
    },
    "sops.defaults.ageKeyFile": "age.key",
    "yaml.schemas": {
        "Kubernetes": "./kubernetes/*.yaml",
        "https://raw.githubusercontent.com/bjw-s/helm-charts/main/charts/library/common/values.schema.json": [
            "**/values.yaml",
        ]
    },
    "vs-kubernetes": {
        "vs-kubernetes.kubeconfig": "./kubeconfig",
        "vs-kubernetes.knownKubeconfigs": [
            "./kubeconfig"
        ]
    }
}
